      {
        "file": "page-builder.css",
        "scope": "page"
      }
    ]
  },
  "dependencies": {
    "blocks": true,
    "font-awesome": true
  },
  "permissions": {
    "create_pages": true,
    "edit_pages": true,
    "delete_pages": true
  }
}
